{
  "name": "@depub/graphql",
  "version": "1.2.0",
  "private": true,
  "description": "Apollo GraphQL server",
  "license": "gpl",
  "author": "0xNaN Team",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "node build.js",
    "deploy:prod": "NODE_ENV=production npx @cloudflare/wrangler publish --env prod",
    "deploy:stag": "npx @cloudflare/wrangler publish",
    "dev": "npx miniflare --watch --live-reload --debug --env .env",
    "gen-graphql-types": "graphql-codegen",
    "start": "npx miniflare --build-watch-path src"
  },
  "dependencies": {
    "@cloudflare/workers-types": "3.3.1",
    "@likecoin/iscn-js": "0.0.7",
    "apollo-datasource": "3.3.1",
    "apollo-server-cloudflare": "3.6.2",
    "apollo-server-errors": "3.3.1",
    "graphql": "16.3.0",
    "types-cloudflare-worker": "1.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.3.1",
    "@esbuild-plugins/node-globals-polyfill": "0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "0.1.4",
    "@graphql-codegen/cli": "2.5.0",
    "@graphql-codegen/typescript": "2.4.3",
    "@graphql-codegen/typescript-resolvers": "2.5.0",
    "esbuild": "0.14.18",
    "typescript": "4.5.5"
  }
}